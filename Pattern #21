for(int i=0;i<n;i++)
    {
      if(i==0||i==n-1)
      {
        for(int j=0;j<n;j++)
        {
          cout<<"*";
        }cout<<endl;
      }
      
      else
      {
        for(int j=0;j<1;j++)
        {
          cout<<"*";
        }
        
        for(int k=0;k<n-2;k++)
        {
          cout<<" ";
        }
        
        for(int l=0;l<1;l++)
        {
          cout<<"*";
        }
        cout<<endl;
      }
